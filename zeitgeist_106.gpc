				polarStick = POLAR_LS;
			} else {
				polarStick = POLAR_RS;	
			}
			break;
		}
	}
	
	// Set value to % the stick is moved (sets of 10)
	polarValue = get_polar(polarStick, POLAR_RADIUS) >> 10;
